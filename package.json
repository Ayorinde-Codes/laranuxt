{
  "name": "laranuxt",
  "version": "1.0.0",
  "description": "laravel+nuxt boilerplate",
  "author": "kevin olson <acidjazz@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "bulma": "node-sass --output-style compressed resources/assets/sass/bulma.sass > resources/static/bulma.css",
    "mdi": "cp node_modules/@mdi/font/css/materialdesignicons.min.css resources/static/mdi.css; cp node_modules/@mdi/font/fonts/* resources/static/fonts/.; cp node_modules/@mdi/font/css/materialdesignicons.min.css.map resources/static/.;"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0"
  },
  "dependencies": {
    "@mdi/font": "^3.2.89",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/proxy": "^1.3.1",
    "bulma": "^0.7.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.3.4"
  }
}
